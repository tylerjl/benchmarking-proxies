worker_processes auto; # default 1
error_log stderr error; # default to logs/error.log

events {
    #use epoll; # by default use the best option
    worker_connections 2500; # default 512
}

http {
    access_log off;

    server {
        listen 0.0.0.0:8080;

        return 200 "Hello, world!";
    }
}
